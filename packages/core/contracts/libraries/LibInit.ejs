pragma solidity ^0.8.17;

import { ISystem } from "solecs/interfaces/ISystem.sol";

library LibInit {
<% systems?.filter(({generateInit}) => !!generateInit).forEach(({name, generateInit}) => { -%>
  function init<%= name %>(ISystem system) internal {
<% if (withGenInits) { -%>
    system.execute(<%- generateInit -%>);
<% } -%>
  }

<% }); -%>
}