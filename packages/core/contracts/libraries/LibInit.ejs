pragma solidity ^0.8.17;

import { ISystem } from "solecs/interfaces/ISystem.sol";

library LibInit {
<% systems?.filter(({initialize}) => !!initialize).forEach(({name, initialize}) => { -%>
  function init<%= name %>(ISystem system) internal {
<% if (withSystemInit) { -%>
    system.execute(<%- initialize -%>);
<% } -%>
  }

<% }); -%>
}